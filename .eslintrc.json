{
  "env": {
    "node": true,
    "jest/globals": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "plugins": ["prettier", "jest"],
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "rules": {
    "curly": "error",
    "eqeqeq": "error",
    "prettier/prettier": "error"
  }
}
